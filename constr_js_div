<script type="text/javascript">
				function fadeinMenubarFrame(v) {
				//var = document.getElementByID("menubarFrame");
				v.style.backgroundColor = "#006600";
				}
				function fadeoutMenubarFrame(v) {
				//var = document.getElementByID("menubarFrame");
				v.style.backgroundColor = "#002400";
				}
				/*if (var.style.backgroundColor != "#06ff06")
				{
					var.style.backgroundColor = "#06ff06";
				}
				function fadeinMenubarFrame() {
				var = document.getElementByID("menubarFrame");
				if (var.style.backgroundColor != "#007100")
				{
					var.style.backgroundColor = "#007100";
				}*/
			</script>
			
			<script type="text/javascript">

					var x = document.getElementById("flashinglightLeft");
					var y = document.getElementById("flashinglightRight");
					
					setInterval(function(){					
							x.style.backgroundColor = "#f5f02e";
							y.style.backgroundColor = "#f5f02e";							
						}, 2000);
					setInterval(function(){				
							x.style.backgroundColor = "#5f5c05";
							y.style.backgroundColor = "#5f5c05";
						}, 2500);

					</script>
					
					<DIV class="header_section_center_bottom">

					<DIV class="menubar" id="menubarFrame" onmouseover="fadeoutMenubarFrame(this)" onmouseout="fadeinMenubarFrame(this)">

					<SPAN style="font-size: 1.1em;font-weight:bold;">This site is under construction. Please check back soon.</SPAN>
					<br/>
					<SPAN style="font-size:1.1em;font-weight:bold;">Catch me at <a href="mailto:alex@alexanderofoedu.com?subject=HiAlex!">alex@alexanderofoedu.com</a> in the meantime.</SPAN>
					<br/>
					<SPAN style="font-size:0.6em;font-weight:normal;text-align:center;">And this DIV Element does a cool little color flick I built. I love stuff like this. Javascript is so awesome!</SPAN>

					</DIV>

				</DIV>
				
